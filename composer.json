{
    "name": "shawnsandy/dashelements",
    "type": "library",
    "description": "Dash elements",
    "keywords": [
        "shawnsandy",
        "dashelements"
    ],
    "homepage": "https://github.com/shawnsandy/dashelements",
    "license": "MIT",
    "authors": [{
        "name": "shawn sandy",
        "email": "shanwsandy04@gmail.com",
        "homepage": "https://github.com/shawnsandy",
        "role": "Developer"
    }],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/contracts": "~5.3|~5.4",
        "illuminate/http": "~5.3|~5.4",
        "illuminate/support": "~5.3|~5.4",
        "shawnsandy/extras": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ShawnSandy\\DashElements\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShawnSandy\\DashElements\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
